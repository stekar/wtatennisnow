<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <stroke
                android:width="1dp"
                android:color="@color/tourna_shape_oval_stroke_selected">
            </stroke>
            <solid
                android:color="@color/tourna_shape_oval_fill_selected">
            </solid>
        </shape>
    </item>

    <item
        android:top="0dp"
        android:left="0dp">
        <bitmap
            android:src="@drawable/ic_action_share"
            android:gravity="center"
            />
    </item>
</layer-list>